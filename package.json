{
  "name": "@nuxtjs/i18n",
  "description": "i18n for Nuxt",
  "version": "8.0.0-alpha.0",
  "homepage": "https://i18n.nuxtjs.org",
  "packageManager": "yarn@3.1.1",
  "bugs": {
    "url": "https://github.com/nuxt-community/i18n-module/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-community/i18n-module.git"
  },
  "license": "MIT",
  "contributors": [
    {
      "name": "Paul Gascou-Vaillancourt (@paulgv)"
    },
    {
      "name": "Rafal Chlodnicki (@rchl)"
    },
    {
      "name": "Kazuya Kawaguchi (@kazupon)"
    }
  ],
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "type": "module",
  "files": [
    "dist",
    "scripts"
  ],
  "scripts": {
    "build": "nuxt-module-build",
    "build:dev": "nuxi build playground",
    "preview:dev": "node playground/.output/server/index.mjs",
    "stub": "nuxt-module-build --stub",
    "dev": "nuxi dev playground",
    "prepare": "yarn stub && nuxi prepare playground",
    "fix": "run-p lint:fix format:fix",
    "format": "prettier --config .prettierrc --ignore-path .prettierignore '**/*.{js,json,html}'",
    "format:fix": "yarn format --write",
    "lint": "eslint --ext .js,.ts,.vue,.json .",
    "lint:fix": "yarn lint --fix",
    "test": "run-s test:unit test:spec",
    "test:unit": "vitest run test",
    "test:spec": "yarn build && vitest run specs"
  },
  "dependencies": {
    "@intlify/shared": "next",
    "@intlify/unplugin-vue-i18n": "latest",
    "@nuxt/kit": "^3.0.0-rc.4",
    "cookie-es": "^0.5.0",
    "debug": "^4.3.2",
    "defu": "^6.0.0",
    "js-cookie": "^3.0.1",
    "knitwork": "^0.1.2",
    "mlly": "^0.5.4",
    "pathe": "^0.3.2",
    "vue-i18n": "beta",
    "vue-i18n-bridge": "beta",
    "vue-i18n-routing": "edge"
  },
  "devDependencies": {
    "@babel/parser": "^7.17.9",
    "@nuxt/module-builder": "latest",
    "@nuxt/schema": "^3.0.0-rc.4",
    "@nuxt/test-utils-edge": "3.0.0-rc.4-27600153.ad42023",
    "@types/debug": "^4.1.7",
    "@types/js-cookie": "^3.0.1",
    "@types/rimraf": "^3",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "debug": "^4.3.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jiti": "^1.14.0",
    "lint-staged": "^12.1.2",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.0.0-rc.4",
    "playwright": "^1.22.2",
    "prettier": "^2.7.0",
    "rimraf": "^3.0.2",
    "ts-essentials": "^9.1.2",
    "typescript": "^4.7.4",
    "vitest": "^0.15.1",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{json,md,yml}": [
      "prettier --write"
    ],
    "*.{js,vue}": [
      "prettier --write",
      "eslint --fix --ext .js,.vue"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "eslint --fix --ext .ts"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "^14.16.0 || ^16.11.0 || ^17.0.0 || ^18.0.0"
  }
}
